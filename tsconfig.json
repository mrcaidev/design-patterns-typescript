{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "experimentalDecorators": true,
    "module": "commonjs",
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "allowJs": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
